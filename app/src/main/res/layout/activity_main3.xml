        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.394" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="270dp"
        android:layout_height="70dp"
        android:text="4.T-shirt bt Migos"
